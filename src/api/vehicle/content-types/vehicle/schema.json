{
  "kind": "collectionType",
  "collectionName": "vehicles",
  "info": {
    "singularName": "vehicle",
    "pluralName": "vehicles",
    "displayName": "vehicle",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "make": {
      "type": "string"
    },
    "model": {
      "type": "string"
    },
    "mileage": {
      "type": "string"
    },
    "color": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "make"
    },
    "stock_number": {
      "type": "integer"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "available",
        "sold",
        "soon"
      ]
    },
    "features": {
      "type": "json",
      "default": []
    },
    "vin": {
      "type": "string"
    },
    "transmission": {
      "type": "string"
    },
    "youtube_link": {
      "type": "string"
    },
    "year": {
      "type": "date"
    },
    "registration_expiry": {
      "type": "date"
    },
    "location": {
      "type": "string"
    },
    "reason_for_sale": {
      "type": "string"
    },
    "number_of_owners": {
      "type": "integer"
    },
    "body_style": {
      "type": "enumeration",
      "enum": [
        "coupe",
        "sedan",
        "hatchback",
        "station_wagon"
      ]
    },
    "engine": {
      "type": "enumeration",
      "enum": [
        "gasoline",
        "diesel",
        "electric",
        "hybrid"
      ]
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "vehicle"
    },
    "description": {
      "type": "text"
    }
  }
}
